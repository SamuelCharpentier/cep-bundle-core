{
	"name": "@samuel-charpentier/cep-bundler-core",
	"version": "0.0.2",
	"description": "Core functionality for making bundler extensions to compile CEP",
	"main": "dist/cep-bundler-core.cjs.js",
	"types": "dist/index.d.ts",
	"module": "dist/cep-bundler-core.esm.js",
	"browser": "dist/cep-bundler-core.umd.js",
	"bin": {
		"cep-bundler-core": "./bin.js"
	},
	"files": [
		"dist/**/*",
		"bin.js"
	],
	"scripts": {
		"dev": "rollup --config --watch",
		"build": "rollup --config"
	},
	"keywords": [
		"Adobe",
		"CEP",
		"ExtendScript",
		"TypeScript",
		"Bundler",
		"Extension",
		"Plugin"
	],
	"author": "Samuel Charpentier",
	"license": "MIT",
	"dependencies": {
		"fs-extra": "^9.0.1",
		"sudo-prompt": "^9.2.1",
		"yargs": "^15.4.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^19.0.0",
		"@types/fs-extra": "^9.0.1",
		"@types/node": "^14.0.14",
		"@typescript-eslint/eslint-plugin": "^4.28.0",
		"@typescript-eslint/parser": "^4.28.0",
		"builtin-modules": "^3.1.0",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"prettier": "^2.0.5",
		"rollup": "^2.19.0",
		"rollup-plugin-node-externals": "^2.2.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-typescript2": "^0.27.1",
		"tape": "^5.0.1",
		"typescript": "^3.9.6",
		"xml-js": "^1.6.11"
	},
	"cep": {
		"out": "./test",
		"name": "Graphics Packager",
		"id": "graphics.packager",
		"version": "1",
		"env": "development",
		"development": {
			"isDev": true,
			"name": "graphics packager DEVELOPMENT",
			"devPort": 5000,
			"debugPorts": {
				"ILST": 6666
			},
			"noSymlink": true
		}
	}
}
