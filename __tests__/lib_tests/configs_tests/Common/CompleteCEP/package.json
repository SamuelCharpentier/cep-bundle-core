{
	"cep": {
		"compileOptions": {
			"outputFolder": "./dist",
			"debugInProduction": false
		},
		"manifest": {
			"extensionBundle": {
				"id": "my.bundle",
				"version": "7.0",
				"name": "Awsome Extensions Bundle",
				"cepVersion": "8.0"
			},
			"authorName": "Samuel Charpentier",
			"contact": "samuel@jaunemoutarde.ca",
			"legal": "https://AwsomeExtensions.com/legal",
			"abstract": "https://AwsomeExtensions.com/legal",
			"executionEnvironment": {
				"localeList": [
					"fr_CA",
					"en_US"
				]
			},
			"extensions": {
				"id": "my.extension",
				"version": "0.0.1",
				"hostList": [
					{
						"host": "Illustrator",
						"version": "ALL",
						"debugPort": "999"
					},
					{
						"host": "InDesign",
						"version": 12,
						"debugPort": "998"
					}
				],
				"dispatchInfo": [
					{
						"extensionData": [
							"This extension is awesome"
						],
						"resources": {
							"htmlPath": "./index.html",
							"scriptPath": "./scripts/main.jsx",
							"cefParams": [
								"--parameter1=value1",
								"--enable-nodejs"
							]
						},
						"lifecycle": {
							"startOn": [
								"applicationActivate",
								"com.adobe.csxs.events.ApplicationActivate"
							]
						},
						"ui": {
							"type": "Panel",
							"menu": {
								"menuName": "My awesome extension"
							},
							"geometry": {
								"maxSize": {
									"width": 400,
									"height": 800
								},
								"minSize": {
									"width": 200,
									"height": 400
								},
								"size": {
									"width": 300,
									"height": 600
								}
							},
							"icons": {
								"normal": "./icons/normal.jpg"
							}
						}
					},
					{
						"extensionData": [
							"This DispatchInfo is for InDesign"
						],
						"host": "InDesign",
						"resources": {
							"htmlPath": "./dst/index.html",
							"scriptPath": "./scripts/main.jsx"
						},
						"ui": {
							"type": "Panel",
							"menu": {
								"menuName": "My awesome extension (InDesign)"
							},
							"geometry": {
								"maxSize": {
									"width": 400,
									"height": 800
								},
								"minSize": {
									"width": 200,
									"height": 400
								},
								"size": {
									"width": 300,
									"height": 600
								}
							},
							"icons": {
								"normal": "./icons/normal.jpg"
							}
						}
					}
				],
				"dependencyList": [
					{
						"id": "my.dependency",
						"version": "0.0.1"
					}
				]
			},
			"compileOptions": {
				"outputFolder": "./dist",
				"debugInProduction": false
			}
		}
	}
}
